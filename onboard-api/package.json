{
  "name": "@centrifuge/onboard-api",
  "private": true,
  "version": "1.0.0",
  "description": "API for the onboarding system of Tinlake",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-format": "prettier --check \"./**/*.{ts,tsx}\"",
    "start": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:migrate": "ts-node src/db/migrate.ts",
    "db:seed": "ts-node src/db/seed.ts"
  },
  "dependencies": {
    "@connectedcars/jwtutils": "^1.0.26",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "concurrently": "^5.3.0",
    "postgres": "2.0.0-beta.2",
    "postgres-shift": "^0.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@types/express": "^4.17.8",
    "prettier-plugin-organize-imports": "^1.1.1",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "tsconfig-paths": "^3.9.0"
  }
}
