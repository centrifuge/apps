{
  "name": "tinlake-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browser": {
    "child_process": false,
    "fs": false
  },
  "scripts": {
    "build": "npm run build:next && npm run build:lambda",
    "build-for-husky": "tsc",
    "dev:next": "PORT=3000 node server.js",
    "build:next": "next build",
    "start": "run-p -l dev:next dev:lambda",
    "serve": "next start",
    "test": "jest",
    "export": "npm run build && next export",
    "lint": "tslint --project .",
    "lint-fix": "tslint --fix --project .",
    "lint-fix-staged": "git diff --staged --name-only | grep -E '\\.tsx?$' | xargs npm run lint-fix && git diff --staged --name-only | grep -E '\\.tsx?$' | xargs git add",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "check-format": "prettier --check \"./**/*.{ts,tsx}\"",
    "dev:lambda": "netlify-lambda serve lambda",
    "build:lambda": "netlify-lambda build lambda"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix-staged && npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centrifuge/tinlake-ui.git"
